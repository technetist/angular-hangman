    "@tailwindcss/postcss": "^4.0.8",
    "cz-git": "^1.11.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.8",
